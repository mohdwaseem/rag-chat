{
"Serilog": {
  "MinimumLevel": {
    "Default": "Information",
    "Override": {
      "Microsoft.AspNetCore": "Warning",
      "System": "Warning"
    }
  },
  "WriteTo": [
    {
      "Name": "Console",
      "Args": {
        "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
      }
    },
    {
      "Name": "File",
      "Args": {
        "path": "Logs/application-.log",
        "rollingInterval": "Day",
        "retainedFileCountLimit": 30,
        "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
      }
    }
  ]
},
"Logging": {
  "LogLevel": {
    "Default": "Information",
    "Microsoft.AspNetCore": "Warning"
  }
},
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200"
    ]
  },
  "GitHub": {
    "Token": "",
    "Endpoint": "https://models.inference.ai.azure.com/",
    "ModelId": "gpt-4o-mini"
  },
  "OpenAI": {
    "ApiKey": "",
    "Model": "gpt-3.5-turbo"
  },
  "Qdrant": {
    "Host": "localhost",
    "Port": "6334",
    "UseHttps": false,
    "CollectionName": "documents"
  },
  "DemoSettings": {
    "ChunkSize": 500,
    "MaxSearchResults": 10,
    "UseGitHubModels": true,
    "UseHeadlessBrowser": true,
    "BrowserTimeout": 30000,
    "AngularWaitTime": 5000,
    "MinRelevanceScore": 0.1,
    "UseQueryExpansion": true,
    "UseSourceDiversity": true
     
   },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 300,
      "WindowMinutes": 1,
      "QueueLimit": 0
    },
    "Chat": {
      "PermitLimit": 30,
      "WindowMinutes": 1,
      "QueueLimit": 0
    }
  }
}